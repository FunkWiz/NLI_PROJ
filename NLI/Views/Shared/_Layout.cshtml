<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>הספרייה הלאומית</title>
    <meta name="viewport" content="width=device-width, user-scalable=no">
    <link href="~/Built/main.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <div id="root"></div>
    @RenderBody()
    <script>
        @Html.Raw(ViewBag.PageData);
        @Html.Raw(ViewBag.BaseUrl);
      @{
          string baseUrl = Url.Content("~/");
          if (baseUrl[baseUrl.Length - 1] != '/')
          {
              baseUrl = string.Format("{0}/", baseUrl);
          }
       }
        window.basePath = '@baseUrl';
    </script>
    <script src="https://code.jquery.com/jquery-3.2.1.min.js"
            integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
            crossorigin="anonymous"></script>
    <script src="~/Built/main.js"></script>
</body>
</html>